{
	"name": "strimoid/strimoid",
	"description": "Strimoid.pl source code",
	"keywords": ["strimoid"],
	"require": {
        "php": ">=5.5.0",
        "ext-apcu": ">=4.0.0",
        "laravel/framework": "5.0.*",
        "intervention/image": "2.0.*",
        "phindmarsh/summon": "dev-master",
        "cohensive/embed": "dev-master",
        "duxet/laravel-rethinkdb": "dev-master",
        "thujohn/rss": "dev-master",
        "rollbar/rollbar": "dev-master",
        "roumen/sitemap": "dev-master",
        "iron-io/iron_mq": "dev-master",
        "jenssegers/agent": "*",
        "jeremykendall/php-domain-parser": "1.3.1",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/oauth-subscriber": "0.1.*",
        "symfony/dom-crawler": "~2.5.6",
        "symfony/css-selector": "~2.5.6",
		"league/fractal": "~0.10@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "barryvdh/laravel-ide-helper": "~1.11@dev",
        "barryvdh/laravel-debugbar": "1.*",
        "fzaninotto/faker": "*"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
            "app/handlers",
            "app/helpers",
            "app/facades"
		],
	  	"psr-4": {
			"Strimoid\\": "app/"
	  	}
	},
    "autoload-dev": {
		"classmap": [
	  		"tests/TestCase.php"
		]
  	},
  	"scripts": {
		"post-install-cmd": [
	  		"php artisan clear-compiled",
	  		"php artisan optimize"
		],
		"post-update-cmd": [
	  		"php artisan clear-compiled",
	  		"php artisan optimize"
		],
		"post-create-project-cmd": [
	  		"php artisan key:generate"
		]
  	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
