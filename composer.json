{
      "name": "strimoid/strimoid",
      "description": "Strimoid.pl source code",
      "type": "project",
      "keywords": ["strimoid"],
      "homepage": "https://strimoid.pl",
      "license": "AGPL-3.0",
      "authors": [
          {
              "name": "duxet",
              "email": "duxetlg@gmail.com",
              "role": "Developer"
          }
      ],
      "support": {
          "irc": "irc://irc.freenode.org/strimoid"
      },
      "require": {
          "php": ">=7.0.8",
          "laravel/framework": "5.3.*",
          "laravelcollective/html": "5.3.*",
          "intervention/image": "^2.3",
          "roumen/sitemap": "^2.6",
          "jeremykendall/php-domain-parser": "^3.0",
          "guzzlehttp/guzzle": "^5.3",
          "guzzlehttp/oauth-subscriber": "^0.2",
          "graham-campbell/markdown": "^6.1",
          "graham-campbell/throttle": "^5.2",
          "jenssegers/agent": "^2.3",
          "laracasts/flash": "^1.3",
          "laracasts/utilities": "^2.1",
          "watson/rememberable": "^2.0",
          "vinkla/pusher": "^2.4",
          "vinkla/algolia": "^2.4",
          "vinkla/hashids": "^2.4",
          "filp/whoops": "^2.1",
          "msurguy/honeypot": "^1.0",
          "lucadegasperi/oauth2-server-laravel": "^5.2",
          "pda/pheanstalk": "^3.1",
          "predis/predis": "^1.1",
          "rcrowe/twigbridge": "^0.9",
          "lord/laroute" : "~2.3",
          "barryvdh/laravel-cors": "^0.8",
          "jenssegers/raven": "^1.8",
          "laravel/socialite": "^2.0",
          "symfony/dom-crawler": "^3.1",
          "symfony/css-selector": "^3.1",
          "anhskohbo/no-captcha": "^2.1",
          "dingo/api": "1.0.x@dev",
          "thomaswelton/laravel-gravatar": "^1.1",
          "syholloway/mrcolor": "^0.0.1",
          "league/glide-laravel": "^1.0",
          "consolidation/robo": "^1.0",
            "talyssonoc/react-laravel": "dev-master"
      },
      "require-dev": {
          "barryvdh/laravel-ide-helper": "^2.2",
          "barryvdh/laravel-debugbar": "^2.2",
          "doctrine/dbal": "^2.5",
          "codeception/codeception": "^2.2",
          "fzaninotto/faker": "^1.6",
          "laracasts/generators": "^1.1",
          "mockery/mockery": "^0.9"
      },
      "autoload": {
          "classmap": [
              "database"
          ],
          "files": [
              "app/utils.php"
          ],
            "psr-4": {
              "Strimoid\\": ["src/", "app/"]
            }
      },
      "scripts": {
          "post-install-cmd": [
              "php artisan clear-compiled",
              "php artisan optimize"
          ],
          "post-update-cmd": [
              "php artisan clear-compiled",
              "php artisan ide-helper:generate -M",
              "php artisan optimize"
          ],
          "post-root-package-install": [
              "php -r \"copy('.env.sample', '.env');\""
          ],
          "post-create-project-cmd": [
              "php -r \"copy('.env.sample', '.env');\"",
              "php artisan key:generate"
          ],
          "test": "vendor/bin/codecept run"
      },
      "config": {
          "preferred-install": "dist"
      },
      "minimum-stability": "dev",
      "prefer-stable": true
  }
