{
    "name": "strimoid/strimoid",
    "description": "Strimoid.pl source code",
    "type": "project",
    "keywords": [
        "strimoid"
    ],
    "homepage": "https://strm.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "duxet",
            "email": "duxetlg@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/strimoid"
    },
    "require": {
        "php": ">=8.2",
        "ext-apcu": "*",
        "laravel/framework": "^10.0",
        "laravelcollective/html": "^6.0",
        "intervention/image": "^2.3",
        "jeremykendall/php-domain-parser": "^5.3",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/oauth-subscriber": "^0.6",
        "graham-campbell/markdown": "^15.0",
        "graham-campbell/throttle": "^10.0",
        "jenssegers/agent": "^2.3",
        "laracasts/flash": "^3.0",
        "laracasts/utilities": "^3.0",
        "watson/rememberable": "^6.0",
        "vinkla/hashids": "^11.0",
        "filp/whoops": "^2.1",
        "msurguy/honeypot": "^1.0",
        "pda/pheanstalk": "^4.0",
        "rcrowe/twigbridge": "^0.14",
        "laravel/socialite": "^5.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/css-selector": "^6.0",
        "syholloway/mrcolor": "^0.0.1",
        "league/glide-laravel": "^1.0",
        "laravel/passport": "^11.2",
        "danielstjules/stringy": "^3.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "tightenco/ziggy": "^1.0",
        "pusher/pusher-php-server": "^7.2",
        "php-http/guzzle7-adapter": "^1.0",
        "mvanduijker/laravel-mercure-broadcaster": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-debugbar": "^3.2",
        "doctrine/dbal": "^3.0",
        "laracasts/generators": "^2.0",
        "mockery/mockery": "^1.0",
        "symplify/easy-coding-standard": "^12.0",
        "nunomaduro/larastan": "^2.0",
        "rector/rector": "^0.18"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/utils.php"
        ],
        "psr-4": {
            "Strimoid\\": [
                "src/",
                "app/"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "Pdp\\Installer::updateLocalCache",
            "@php artisan ide-helper:generate -M",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"copy('.env.sample', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"copy('.env.sample', '.env');\"",
            "@php artisan key:generate"
        ],
        "test": "vendor/bin/codecept run"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
