{
	"name": "strimoid/strimoid",
	"description": "Strimoid.pl source code",
	"keywords": ["strimoid"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/duxet/oauth2-server-php"
        }
    ],
	"require": {
        "laravel/framework": "4.2.*",
        "bshaffer/oauth2-server-php": "dev-mongo",
        "bshaffer/oauth2-server-httpfoundation-bridge": "v1.0",
        "intervention/image": "2.0.*",
        "phindmarsh/summon": "dev-master",
        "cohensive/embed": "dev-master",
        "jenssegers/mongodb": "dev-master",
        "jenssegers/mongodb-session": "*",
        "thujohn/rss": "dev-master",
        "rollbar/rollbar": "dev-master",
        "intouch/laravel-newrelic": "*",
        "roumen/sitemap": "dev-master",
        "iron-io/iron_mq": "dev-master",
        "jenssegers/agent": "*",
        "jeremykendall/php-domain-parser": "1.3.1",
        "fitztrev/laravel-html-minify": "1.0.3",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/oauth-subscriber": "0.1.*",
        "symfony/dom-crawler": "~2.5.6",
        "symfony/css-selector": "~2.5.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "~1.11@dev",
        "barryvdh/laravel-debugbar": "1.*",
        "satooshi/php-coveralls": "*",
        "fzaninotto/faker": "*"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
            "app/handlers",
            "app/helpers",
            "app/facades",
            "app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
