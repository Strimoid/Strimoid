{
	"name": "strimoid/strimoid",
	"description": "Strimoid.pl source code",
    "type": "project",
	"keywords": ["strimoid"],
    "homepage": "https://strimoid.pl",
    "license": "AGPL-3.0",
    "authors": [
	    {
	  	    "name": "duxet",
		    "email": "duxetlg@gmail.com",
		    "role": "Developer"
	    }
    ],
    "support": {
	    "irc": "irc://irc.freenode.org/strimoid"
	},
	"require": {
        "php": ">=5.5.0",
        "laravel/framework": "5.0.*",
	    "illuminate/html": "5.0.*",
	    "jenssegers/mongodb": "dev-L5",
        "intervention/image": "2.0.*",
        "phindmarsh/summon": "dev-master",
        "cohensive/embed": "dev-master",
        "thujohn/rss": "dev-master",
        "rollbar/rollbar": "dev-master",
        "roumen/sitemap": "dev-master",
        "iron-io/iron_mq": "dev-master",
        "jenssegers/agent": "*",
        "jeremykendall/php-domain-parser": "1.4.*",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/oauth-subscriber": "0.1.*",
        "symfony/dom-crawler": "~2.5.6",
        "symfony/css-selector": "~2.5.6",
		"league/fractal": "~0.10@dev",
	    "league/commonmark": "0.5.*@dev",
	    "pda/pheanstalk": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "barryvdh/laravel-ide-helper": "~1.11@dev",
        "barryvdh/laravel-debugbar": "2.*",
        "fzaninotto/faker": "*",
	    "behat/behat": "~3.0@dev",
		"behat/mink": "~1.6@dev",
		"behat/mink-extension": "~2.0@dev",
	    "laracasts/behat-laravel-extension": "dev-master"
    },
  	"repositories": [
	  	{
			"type": "vcs",
			"url": "https://github.com/duxet/laravel-mongodb"
	  	}
  	],
	"autoload": {
		"classmap": [
		    "database"
		],
	    "files": [
		    "app/utils.php"
	    ],
	  	"psr-4": {
			"Strimoid\\": "app/"
	  	}
	},
    "autoload-dev": {
		"classmap": [
	  		"tests/TestCase.php"
		]
  	},
  	"scripts": {
		"post-install-cmd": [
	  		"php artisan clear-compiled",
	  		"php artisan optimize"
		],
		"post-update-cmd": [
	  		"php artisan clear-compiled",
	  		"php artisan optimize"
		],
		"post-create-project-cmd": [
	  		"php artisan key:generate"
		]
  	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
