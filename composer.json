{
    "name": "strimoid/strimoid",
    "description": "Strimoid.pl source code",
    "type": "project",
    "keywords": [
        "strimoid"
    ],
    "homepage": "https://strm.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "duxet",
            "email": "duxetlg@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/strimoid"
    },
    "require": {
        "php": ">=7.3.0",
        "laravel/framework": "^7.0",
        "laravelcollective/html": "^6.0",
        "intervention/image": "^2.3",
        "laravelium/sitemap": "^7.0",
        "jeremykendall/php-domain-parser": "^5.3",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/oauth-subscriber": "^0.3",
        "graham-campbell/markdown": "^11.1",
        "graham-campbell/throttle": "^7.0",
        "jenssegers/agent": "^2.3",
        "laracasts/flash": "^3.0",
        "laracasts/utilities": "^3.0",
        "watson/rememberable": "^4.0",
        "vinkla/hashids": "^8.0",
        "filp/whoops": "^2.1",
        "msurguy/honeypot": "^1.0",
        "pda/pheanstalk": "^4.0",
        "rcrowe/twigbridge": "^0.11",
        "laravel/socialite": "^4.3",
        "symfony/dom-crawler": "^5.0",
        "symfony/css-selector": "^5.0",
        "dingo/api": "^3.0",
        "thomaswelton/laravel-gravatar": "^1.1",
        "syholloway/mrcolor": "^0.0.1",
        "league/glide-laravel": "^1.0",
        "laravel/passport": "^7.0",
        "danielstjules/stringy": "^3.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "tightenco/ziggy": "^0.9",
        "gliterd/laravel-backblaze-b2": "^1.2",
        "meilisearch/meilisearch-laravel-scout": "^0.10.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-debugbar": "^3.2",
        "doctrine/dbal": "^2.5",
        "codeception/codeception": "^4.0",
        "fzaninotto/faker": "^1.6",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.0",
        "symplify/easy-coding-standard": "^7.2",
        "nunomaduro/larastan": "^0.5",
        "codeception/module-laravel5": "^1.0",
        "codeception/module-rest": "^1.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "rector/rector": "^0.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/utils.php"
        ],
        "psr-4": {
            "Strimoid\\": [
                "src/",
                "app/"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "Pdp\\Installer::updateLocalCache",
            "@php artisan ide-helper:generate -M",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"copy('.env.sample', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"copy('.env.sample', '.env');\"",
            "@php artisan key:generate"
        ],
        "test": "vendor/bin/codecept run"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
